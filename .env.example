# OMEGA SDK Configuration
# Copy this file to .env and fill in your values

# Federation Core connection
OMEGA_FEDERATION_URL=http://localhost:9405

# Authentication (optional)
OMEGA_API_KEY=your-api-key-here

# Default context
OMEGA_TENANT_ID=acme
OMEGA_ACTOR_ID=your-actor-id

# Client behavior
OMEGA_TIMEOUT_MS=120000
OMEGA_MAX_RETRIES=3
